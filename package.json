{
  "name": "sveltedevops",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build && touch build/.nojekyll",
    "deploy": "npx gh-pages -d build -t true",
    "fullDeploy": "npm run excelMCDM && npm run build && npm run deploy",
    "fullDeployAndAmmend": "npm run fullDeploy && npm run amend",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "amend": "git add . && git commit --amend --no-edit && git push --force",
    "excelMCDM": "npm run convertAllExcels && cd mcdm && ./venv/bin/python main.py",
    "convertAllExcels": "npm run convertChats && npm run convertCIs && npm run convertVCs",
    "convertChats": "convert-excel-to-json --sourceFile=./src/lib/excels/Chat_Comps.xlsx --config='{\"header\":{\"rows\":1}, \"columnToKey\":{\"*\": \"{{columnHeader}}\"} }' > ./src/lib/jsons/Chats.json",
    "convertCIs": "convert-excel-to-json --sourceFile=./src/lib/excels/CI_Comps.xlsx --config='{\"header\":{\"rows\":1}, \"columnToKey\":{\"*\": \"{{columnHeader}}\"} }' > ./src/lib/jsons/CIs.json",
    "convertVCs": "convert-excel-to-json --sourceFile=./src/lib/excels/VC_Comps.xlsx --config='{\"header\":{\"rows\":1}, \"columnToKey\":{\"*\": \"{{columnHeader}}\"} }' > ./src/lib/jsons/VCs.json"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.20.1",
    "@types/cookie": "^0.5.1",
    "convert-excel-to-json": "^1.7.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.1",
    "svelte-bootstrap-icons": "^2.3.1",
    "svelte-check": "^2.10.3",
    "typescript": "^4.9.5",
    "vite": "^4.3.9"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.3.0",
    "gh-pages": "^4.0.0",
    "gridjs": "^5.1.0",
    "gridjs-svelte": "^2.1.1",
    "mathjs": "^11.8.0",
    "svelte-chartjs": "^3.1.2",
    "svelte-multiselect": "^8.6.2",
    "sveltestrap": "github:laxadev/sveltestrap"
  }
}